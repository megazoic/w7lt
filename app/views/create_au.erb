<div>
  <h3>Select Member for Authorized User Role</h3>
</div>
<div>
	<table>
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Call Sign</th>
			<th>email</th>
			<th></th>
		</tr>
		<% for member in @sel_au_from_mbrs %>
		<tr>
		<td><%=member[:fname]%></td>
		<td><%=member[:lname]%></td>
		<td><%=member[:callsign]%></td>
		<td><%=member[:email]%></td>
		<td><a href="/a/auth_user/role/set/<%= member[:id]%>">select</a></td>
		</tr>
		<% end %>
	</table>
</div>
<div id="footer">
  <p><b>A Ruby Sinatra App</b>.</p>
</div>