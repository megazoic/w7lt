  <h1>PARC Home</h1>
  <p>Please choose...</p>
    <form action="/logout" method="POST">
      <input type="submit" value="Sign Out" />
    </form>
  <a href="/dump">Output the member data as an unfomatted comma separated list</a><br />
  <a href="/query">Query the member data</a><br />
  <a href="/list/members">List members</a><br />
  <a href="/new/member">Add a new member</a><br />
<p>Or choose a member to view from the following list</p><br />
	<table>
		<%@member_lnames.each_slice(5) do |member| %>
		<tr>
		<td><%if !member[0].nil?%><a href="/show/member/<%=member[0].id%>"><%=member[0].lname%><%end%></td>
		<td><%if !member[1].nil?%><a href="/show/member/<%=member[1].id%>"><%=member[1].lname%><% end %></td>
		<td><%if !member[2].nil?%><a href="/show/member/<%=member[2].id%>"><%=member[2].lname%><% end %></td>
		<td><%if !member[3].nil?%><a href="/show/member/<%=member[3].id%>"><%=member[3].lname%><% end %></td>
		<td><%if !member[4].nil?%><a href="/show/member/<%=member[4].id%>"><%=member[4].lname%><% end %></td>
		</tr>
		<%end%>
	</table>
