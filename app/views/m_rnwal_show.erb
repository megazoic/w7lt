</br>
<h4>Member renewal events</h4>
<div>
	<table>
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Recorded By</th>
			<th>Record Type</th>
			<th>Notes</th>
			<th>Date</th>
		</tr>
		<% @renewals.each do |r| %>
			<tr>
				<td><%=r[:fname]%></td>
				<td><%=r[:lname]%></td>
				<td><%=r[:recorded_by]%></td>
				<td><%=r[:event_type]%></td>
				<td><%=r[:notes][0,20]%></td>
				<td><%=r[:ts].strftime("%Y, %b, %d")%></td>
				<%if session[:auth_user_roles].include?('mbr_mgr')%><td><a href="/m/mbr_renewals/edit/<%= r[:id]%>">edit</a> | <a href="/m/mbr_renewals/destroy/<%= r[:id]%>">delete</a></td><%end%>
			</tr>
		<% end %>
	</table>
</div>
