<%#
:fname	:lname	:email	:city	:street
:zip	:state	:callsign	:phm	:phm_pub
:phh	:phh_pub	:phw	:phw_pub
:license_class	:mbr_type	:paid_up	:arrl
:arrl_expire	:ares	:net	:ve	:elmer :sota
%>
<div>
  <h3>Members List: total number listed here: <%=@member.count%></h3>
  <%if @qset%>
  <p>Filters in place</p>
	  <%@qset.each do |k,v|%>
    	<p><%=k%> : value <%=v%></p>
  	  <%end%>
  <%end%>
</div>
<div>
	<table>
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>call</th>
			<th>paid <%=Time.new.year%></th>
			<th></th>
		</tr>
		<% for member in @member %>
		<tr>
		<td><%=member[:fname]%></td>
		<td><%=member[:lname]%></td>
		<td><%=member[:callsign]%></td>
		<td><%=member[:paid_up]%></td>
		<td><a href="/r/show/member/<%= member[:id]%>">view</a><%if session[:auth_user_roles].include?('mbr_mgr')%> | <a href="/m/edit/member/<%= member[:id]%>">edit</a><%end%><%if session[:auth_user_roles].include?('auth_u')%> | <a href="/m/payment/new/<%= member[:id]%>">pay</a><%end%></td>
		</tr>
		<% end %>
	</table>
</div>
